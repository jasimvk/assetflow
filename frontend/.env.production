# Production Environment Variables for Vercel
# These will be set in Vercel Dashboard

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Backend URL (will be your backend deployment URL)
NEXT_PUBLIC_API_URL=https://your-backend-url.com

# Azure AD / Microsoft Entra ID (Optional)
NEXT_PUBLIC_AZURE_CLIENT_ID=your_azure_client_id_here
NEXT_PUBLIC_AZURE_TENANT_ID=your_azure_tenant_id_here
NEXT_PUBLIC_AZURE_REDIRECT_URI=https://your-vercel-domain.vercel.app
